{
	"name": "WeatherAPI",
	"properties": {
		"parameters": {
			"latitude": {
				"type": "string"
			},
			"longitude": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "HttpServer",
		"typeProperties": {
			"url": "@{concat('https://api.open-meteo.com/v1/forecast?latitude=',linkedService().latitude,'&longitude=',linkedService().longitude,'&hourly=temperature_2m&timezone=Europe%2FBerlin')}",
			"enableServerCertificateValidation": true,
			"authenticationType": "Anonymous"
		}
	}
}